---
import { useStoryblokApi } from '@storyblok/astro'
import StoryblokComponent from '@storyblok/astro/StoryblokComponent.astro'

import '../assets/styles/reset.scss';
 
const storyblokApi = useStoryblokApi()
const { data } = await storyblokApi.get('cdn/stories/home', {
  version: 'published',
})
 
const story = data.story
---

<html lang="pt-BR">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Fefa Cap — Tatuagem em Cicatriz</title>

        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="Tatuagem Artística e Cobertura de CICATRIZES e ESTRIAS. Atendimento personalizado e exclusivo feito pela tatuadora Maria Fernanda Capeletti Bisetto, mais conhecida como Fefa. Para informações e agendamentos favor contatar o WhatsApp: 011 93337-6100 Instagram: @fefacap Tiktok: @fefacaptattoo">
        <meta name="keywords" content="Tatuagem, cicatriz, estrias, tatuadora">
        <meta property="og:locale" content="pt_BR">
        <meta property="og:type" content="website">
        <meta property="og:site_name" content="Fefa Cap">
        <meta property="og:description" content="Tatuagem Artística e Cobertura de CICATRIZES e ESTRIAS. Atendimento personalizado e exclusivo feito pela tatuadora Maria Fernanda Capeletti Bisetto, mais conhecida como Fefa. Para informações e agendamentos favor contatar o WhatsApp: 011 93337-6100 Instagram: @fefacap Tiktok: @fefacaptattoo">
        <meta property="og:title" content="Fefa Cap">
        <meta property="og:url" content="https://fefacap.com">
        <meta property="og:image" content="https://fefacap.com/capa_fefacap.webp">
